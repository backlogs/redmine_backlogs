<div class="model sprint" id="<%= sprint_html_id_or_empty(sprint) %>">
  <div class="id">
    <div class="t"><%= sprint_link_or_empty(sprint) %></div>
    <div class="v"><%= id_or_empty(sprint) %></div>
  </div>
  <div class="name editable" fieldname="name" fieldlabel="<%=l(:field_name)%>"><%= strip_tags sprint.name %></div>
  <div class="sprint_start_date editable date" fieldname="sprint_start_date" fieldlabel="<%=l(:field_sprint_start_date)%>"><%= sprint.sprint_start_date || l(:label_no_start) %></div>
  <div class="effective_date editable date" fieldname="effective_date" fieldlabel="<%=l(:field_effective_date)%>"><%= sprint.effective_date || l(:label_no_end) %></div>
  <div class="description editable" fieldname="description" fieldlabel="<%=l(:field_description)%>" fieldtype="textarea"><%= strip_tags sprint.description %></div>
  <div class="meta">
    <%= render :partial => "shared/model_errors", :object => sprint.errors %>
  </div>
</div>
